# The_Colonel Environment Variables
# Copy this file to .env and edit with your actual values

# OpenAI API Key (required for GPT models)
OPENAI_API_KEY=your-openai-api-key-here

# Anthropic API Key (for Claude models)
ANTHROPIC_API_KEY=your-anthropic-api-key-here

# Server Configuration
DEFAULT_PROFILE=The_Colonel.py
SERVER_HOST=0.0.0.0
SERVER_PORT=8264
AUTH_TOKEN=your-secure-auth-token-here

# Other LLM API Keys (uncomment as needed)
# AZURE_API_KEY=your-azure-api-key-here
# AZURE_API_BASE=your-azure-endpoint-here
# AZURE_API_VERSION=your-azure-api-version-here
# GROQ_API_KEY=your-groq-api-key-here
# TOGETHER_API_KEY=your-together-api-key-here

# General Settings
DISABLE_TELEMETRY=true
OFFLINE_MODE=false
AUTO_RUN=false
SAFE_MODE=off
VERBOSE=false